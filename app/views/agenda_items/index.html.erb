<h1>Listing agenda_items</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Meeting</th>
    <th>Completed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agenda_items.each do |agenda_item| %>
  <tr>
    <td><%= agenda_item.name %></td>
    <td><%= agenda_item.description %></td>
    <td><%= agenda_item.meeting_id %></td>
    <td><%= agenda_item.completed %></td>
    <td><%= link_to 'Show', agenda_item %></td>
    <td><%= link_to 'Edit', edit_agenda_item_path(agenda_item) %></td>
    <td><%= link_to 'Destroy', agenda_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Agenda item', new_agenda_item_path %>
